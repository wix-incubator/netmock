{
  "name": "netmock",
  "version": "1.0.0",
  "description": "A javascript network mocker for tests",
  "main": "index.js",
  "scripts": {
    "lint": "tsc --noEmit && eslint '{**/*,*}.{js,ts}'",
    "lint:fix": "eslint '{**/*,*}.{js,ts}' --fix",
